<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="id" value="java.sql.Types.BIGINT" dbms="postgresql,h2"/>
    <property name="string255" value="java.sql.Types.VARCHAR(255)" dbms="postgresql,h2"/>
    <property name="boolean" value="java.sql.Types.BOOLEAN" dbms="postgresql,h2"/>

    <include file="changelog/person.xml" relativeToChangelogFile="true"/>
    <include file="changelog/vendor.xml" relativeToChangelogFile="true"/>
    <include file="changelog/institution.xml" relativeToChangelogFile="true"/>
    <include file="changelog/food.xml" relativeToChangelogFile="true"/>
    <include file="changelog/donation.xml" relativeToChangelogFile="true"/>
    <include file="changelog/donation_item.xml" relativeToChangelogFile="true"/>

</databaseChangeLog>